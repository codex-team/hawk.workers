{% macro pluralize_ru(n, forms) %}
    {% if n % 100 >= 11 and n % 100 <= 19 %}
        {{ forms[2] }}
    {% else %}
        {% set last = n % 10 %}
        {% if last == 1 %}
            {{ forms[0] }}
        {% elseif last >= 2 and last <= 4 %}
            {{ forms[1] }}
        {% else %}
            {{ forms[2] }}
        {% endif %}
    {% endif %}
{% endmacro %}